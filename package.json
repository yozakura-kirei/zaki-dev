{
  "name": "zaki-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.local next dev -p 8888",
    "dev-pre": "env-cmd -f .env.dev next dev -p 8888",
    "prd": "env-cmd -f .env.prd next dev -p 8888",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:prd": "env-cmd -f .env.prd next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "14.1.2",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "sass": "^1.71.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.2",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
