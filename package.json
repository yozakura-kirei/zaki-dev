    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.2",
    "eslint-config-prettier": "^9.1.0",
