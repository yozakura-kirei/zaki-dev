    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.8",
    "@types/highlight.js": "^10.1.0",
    "dayjs": "^1.11.10",
    "dompurify": "^3.0.11",
    "graphql": "^16.8.1",
    "highlight.js": "^11.9.0",
    "markdown-it": "^14.1.0",
    "next": "14.1.2",
    "open-graph-scraper": "^6.5.1",
